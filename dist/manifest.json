{
  "name": "Image Downloader",
  "description": "by Murad and Hui",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["./background.js"]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "tabs"
  ]
}